PROJECT_NAME        = QtDocxTemplate
PROJECT_NUMBER      = @PROJECT_VERSION@
OUTPUT_DIRECTORY    = @CMAKE_BINARY_DIR@/docs
GENERATE_HTML       = YES
GENERATE_LATEX      = NO
INPUT               = @CMAKE_SOURCE_DIR@/include
FILE_PATTERNS       = *.hpp
RECURSIVE           = YES
EXTRACT_ALL         = NO
EXTRACT_PRIVATE     = NO
EXTRACT_STATIC      = NO
WARNINGS            = YES
WARN_IF_UNDOCUMENTED= NO
WARN_AS_ERROR       = YES
QUIET               = NO
JavadocAutobrief    = YES
